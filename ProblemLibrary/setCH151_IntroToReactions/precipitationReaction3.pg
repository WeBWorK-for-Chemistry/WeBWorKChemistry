$mp1 = MultiAnswer($react1, $react2)->with(
   singleResult => 0,
   separator => " + ",
   tex_separator => " + ",
   checker => sub {
     my $correct= shift; my $student = shift;
     my ($ca,$cb) = @$correct;
     my ($sa,$sb) = @$student;
    if ( ($ca == $sa && $cb == $sb) ||
           ($ca == $sb && $cb == $sa) ) {
          return [1,1];
      } else {
          if ($ca == $sa || $cb == $sa) {
              return [1,0];
          } elsif ($cb == $sb || $ca == $sb) {
              return [0,1];
          } else {
              return [0,0];
