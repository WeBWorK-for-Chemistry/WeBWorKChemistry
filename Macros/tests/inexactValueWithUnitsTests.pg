   sub addTabs {
      my $level = shift;
      my $output='';
      for ($l = 0; $l < $level; $l++){
         $output .='&#9;'
      }
      return $output;
   }

   sub hashPrint {
      my $hashRef = shift;
      my $level = shift;
      my %hash = %$hashRef;
      my $output=addTabs($level) . '{<br/>';
      
      while(my($k, $v) = each %hash) { 
         if (ref($v) eq 'HASH'){
            #$output .= ref($v) . '<br/>';
            $output .= addTabs($level+1) . "$k: <br/>" . hashPrint($v, $level+1) . '}';
         } elsif (ref($v) eq 'ARRAY') {
            $output .= addTabs($level+1) . "$k: " . arrayPrint($v);
         } else {
            $output .= addTabs($level+1) . "$k: $v<br/>";
         }
      }
      return $output . addTabs($level) . '}';
   }
   sub arrayPrint {
      my $arrayRef = shift; 
      my $level = shift;
      my @array = @$arrayRef;
      my $output=addTabs($level) . '[';
      foreach my $a (@array) {
         if (ref($a) eq 'HASH'){
            $output .= addTabs($level) . '<br/>' . hashPrint($a, $level+1) . ', ' ;
         }elsif (ref($v) eq 'ARRAY') {
            $output .= addTabs($level) . arrayPrint($a) . ', ';
         } else {
            $output .= addTabs($level) . $a . ', ';
         }
      }
      return $output . '],<br/>';
   }

   foreach $dataItem (@data){
